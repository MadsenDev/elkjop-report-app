{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "productName": "elkjop-report-app",
  "version": "1.0.0",
  "identifier": "dev.madsens.elkjopreport",
  "build": {
    "beforeBuildCommand": "npm run build",
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:5173",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Elkj√∏p Daily Report",
        "width": 1200,
        "height": 900,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null,
      "capabilities": ["fs-access"]
    }
  },
  "bundle": {
    "active": true,
    "targets": ["deb", "rpm", "msi"],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "fs": {
      "requireLiteralLeadingDot": false
    }
  }
}